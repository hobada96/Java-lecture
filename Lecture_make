import java.io.InputStream;
import java.util.Scanner;

public class Tes1 {

	public static void main(String[] args) {
			Scanner sc = new Scanner(System.in);
			int n[] = new int[5];
			for (int i = 0; i < n.length;i++) {
				n[i] = sc.nextInt();
			}
					
			System.out.println(sum(n));
	}

	private static int sum(int[] n) {
		int sum = 0;
		for (int i : n) {
			sum = sum+i;
		}
		return (sum);
	}
}


